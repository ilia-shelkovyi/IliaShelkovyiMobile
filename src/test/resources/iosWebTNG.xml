<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iOS Web mobile testing Suite for EPAM Mobile Cloud">
    <parameter name="appType" value="web"></parameter>
    <!--<parameter name="udid" value="00008020-001429081A43002E"></parameter>-->
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="browserName" value="Safari"></parameter>
<test name="iOS Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>